! Settings

Xft.autohint:                 0
Xft.lcdfilter:                lcddefault
Xft.hintstyle:                hintfull
Xft.hinting:                  1
Xft.antialias:                1
Xft.rgba:                     rgb

! URLlauncher settings
URxvt*urlLauncher:            /usr/bin/firefox
URxvt*matcher.button:         1

! Related to the scratch pad (i3-wm)
scratchpad*scrollBar:         false
scratchpad*externalBorder:    false
scratchpad*internalBorder:    false

! colours
!black
*color0:        #1c1c1c
*color8:        #4d4d4d
!red
*color1:        #D81860
*color9:        #F00060
!green
*color2:        #60FF60
*color10:       #70FF70
!yellow
*color3:        #f9fd75
*color11:       #f9fd75
!blue
*color4:        #4695c8
*color12:       #4e98c8
!magenta
*color5:        #a78edb
*color13:       #ae99db
!cyan
*color6:        #43afce
*color14:       #50b2ce
!white
*color7:        #f3ebe2
*color15:       #eeeeee

! URxvt settings
! They say it is useful to use this name when you use tmux.
URxvt*termName:                       screen-256color
